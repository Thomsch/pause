<?xml version="1.0" encoding="UTF-8"?>

<?import ch.thomsch.pause.ui.decoration.RegularWindowDecoration?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<VBox xmlns:fx="http://javafx.com/fxml/1"
      alignment="CENTER"
      minWidth="400.0"
      onKeyPressed="#onEnterPressed"
      spacing="5"
      xmlns="http://javafx.com/javafx/8.0.60"
      fx:controller="ch.thomsch.pause.ui.about.AboutController">
    <RegularWindowDecoration text="Closes the window"/>
    <Label id="about-app-title" text="PAUSE"/>
    <Label id="about-licence" text="Licence : GPL3"/>
    <Hyperlink id="about-github" fx:id="gitHubLink" onAction="#onGitHubLinkClick" text="https://github.com/Thomsch"/>
    <stylesheets>
        <URL value="@/stylesheet.css"/>
    </stylesheets>
</VBox>
